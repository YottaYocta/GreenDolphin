{
  "name": "greendolphin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron-compiled/main.cjs",
  "build": {
    "appId": "com.ryanwqiu.greendolphin",
    "files": [
      "dist/**/*",
      "dist-electron-compiled/**/*"
    ],
    "directories": {
      "output": "dist-electron"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:electron": "npm run build && tsc -p tsconfig.electron.json && mv dist-electron-compiled/main.js dist-electron-compiled/main.cjs && electron-builder --mac"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.8",
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "^37.3.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.1.4"
  }
}
